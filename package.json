{
  "name": "vue-hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "serve": "nano-server 5000 dist",
    "test:unit": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service test:unit",
    "lint": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3",
    "register-service-worker": "~1.7.2",
    "vue": "~2.6.14",
    "vue-class-component": "~7.2.6",
    "vue-router": "~3.4.9",
    "vuetify": "~2.6.8",
    "vuex": "~3.6.0"
  },
  "devDependencies": {
    "@mdi/font": "^7",
    "@types/mocha": "^9",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "~4.11.0",
    "@vue/cli-plugin-babel": "^4",
    "@vue/cli-plugin-eslint": "^4",
    "@vue/cli-plugin-pwa": "^4",
    "@vue/cli-plugin-router": "^4",
    "@vue/cli-plugin-typescript": "^4",
    "@vue/cli-plugin-unit-mocha": "^4",
    "@vue/cli-plugin-vuex": "^4",
    "@vue/cli-service": "^4",
    "@vue/compiler-dom": "^3.2.45",
    "@vue/eslint-config-prettier": "~6.0.0",
    "@vue/eslint-config-typescript": "~11.0.0",
    "@vue/test-utils": "^1",
    "eslint": "^8",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-vue": "^7",
    "nano-server": "^0.5.4",
    "prettier": "~2.7.1",
    "sass": "~1.30.0",
    "sass-loader": "~10.1.0",
    "stylus": "~0.58.1",
    "stylus-loader": "^4",
    "typescript": "^4",
    "vue-cli-plugin-pug": "~2.0.0",
    "vue-cli-plugin-vuetify": "~2.0.8",
    "vue-property-decorator": "~9.1.2",
    "vue-template-compiler": "~2.6.14",
    "vuetify-loader": "~1.7.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
